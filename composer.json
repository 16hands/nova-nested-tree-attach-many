{
    "name": "norhan-elnezamy/nova-nested-tree-attach-many",
    "description": "Nova vue-tree-select field for management nested category relations.",
    "keywords": [
        "laravel",
        "nova",
        "belongs-to-many",
        "nested-sets",
        "sorting tree"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "norhan-elnezamy/laravel-nestedset": "6.3"
    },
    "autoload": {
        "psr-4": {
            "PhoenixLib\\NovaNestedTreeAttachMany\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PhoenixLib\\NovaNestedTreeAttachMany\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:norhan-elnezamy/laravel-nestedset.git",
            "reference": "v5"
        }
    ]
}
